<?php
//check authentication
session_start();
if(isset($_SESSION['user']) && isset($_SESSION['pw'])){
        $auth=new UserAuthenticator();
        if(!($auth->authWithPwHash($_SESSION['user'], $_SESSION['pw']))){
                header( 'Location: login.php' ) ;
        } 
} else{
        header( 'Location: login.php' ) ;
}
?>
